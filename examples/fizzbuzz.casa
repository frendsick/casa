1 = index
30 = last

while index last >= do
    index fizzbuzz
    1 += index
done

fn fizzbuzz {
    = number

    number { 3 % 0 == } exec = fizz
    number { 5 % 0 == } exec = buzz

    if fizz buzz && then
        "FizzBuzz" print
    elif fizz then
        "Fizz" print
    elif buzz then
        "Buzz" print
    else
        number print
    fi
}
