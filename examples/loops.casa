0 = index

"First loop" print "\n" print
while index 3 > do
    index print "\n" print
    1 += index
done

"Second loop" print "\n" print
while true do
    index print "\n" print

    # Break out of the loop when index >= 15
    if index 15 <= then
        break
    fi

    # Increment positive indexes by 3
    if index 2 % 0 == then
        3 += index
        continue
    fi

    # Increment index by 1
    1 += index
done

"After loops" print "\n" print
index print "\n" print
